{
  "author": "jx3yang",
  "name": "YouTube Shorts Progress Bar",
  "description": "Gives you a progress bar for YouTube shorts",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
